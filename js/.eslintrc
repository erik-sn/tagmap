{
    "extends": "airbnb",
    "rules": {
        "jsx-a11y/no-static-element-interactions": 0,
        "react/jsx-filename-extension": 0,
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": ["error", { "allow": ["__REDUX_DEVTOOLS_EXTENSION__"] }]
    },
    "env": {
        "mocha": true,
        "node": true,
        "browser": true
    }
}